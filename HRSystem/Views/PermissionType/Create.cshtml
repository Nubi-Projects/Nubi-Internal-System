@model HRSystem.Models.PermissionType
@{
    ViewBag.Title = "Create";
}

<h2>@Resources.NubiHR.PermissionType</h2>

@using (Html.BeginForm(null, null, FormMethod.Post))
{
    <div class="form-group">
       
        <div>
            @Resources.NubiHR.PermissionType
        </div>
        <div>
            @Html.EditorFor(model => model.Type, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
    <br />
    <div class="form-group">
        <div>
            <button class="btn-Back"><a href="@Url.Action("PermissionType","Index")">@Resources.NubiHR.Back</a></button>
            <input type="submit" value=@Resources.NubiHR.Send class="btn2" />
        </div>
    </div>
}